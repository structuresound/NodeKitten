name: "nodeKitten"
target: "static"
build:
	"mac ios":
		sources: matching: ['core/**/*.cpp', '!core/**/*tests*.cpp', '!core/platform/android/**/*.*', '!core/platform/emscripten/**/*.*']
	with: "cmake"
	cFlags:
		std: "c++14"
path:
  install:
    headers: [
      from: 'source/core'
      to: 'include/nodeKitten'
    ]
